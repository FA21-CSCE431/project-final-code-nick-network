<% if current_user.uid == "110391296899575374577" %>
  <h1>Editing Meeting</h1>

  <%= render 'form', meeting: @meeting %>

  <div class="container", style="margin-left: 12em;">
    <%= link_to '<button type="button"> View </button>'.html_safe, @meeting %>
    <%= link_to '<button type="button"> Back </button>'.html_safe, meetings_path %>
  </div>
<% else %>
  <center>
  <h1>You're not an admin!</h1>
  <%= link_to 'Back', meetings_path %>
  </center>
<% end %>
